{
  "proximity_window": 15,
  "phi_identifiers": {
    "patterns": [
      {
        "name": "DOB_MMDDYYYY",
        "regex": "\\b(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/\\d{4}\\b",
        "category": "DOB"
      },
      {
        "name": "DOB_MMDDYY",
        "regex": "\\b(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/\\d{2}\\b",
        "category": "DOB"
      },
      {
        "name": "DOB_YYYYMMDD",
        "regex": "\\b\\d{4}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])\\b",
        "category": "DOB"
      },
      {
        "name": "SSN",
        "regex": "\\b\\d{3}-\\d{2}-\\d{4}\\b",
        "category": "SSN"
      },
      {
        "name": "MRN",
        "regex": "\\b\\d{6,10}\\b",
        "category": "MRN",
        "context_keywords": ["mrn", "medical record number", "record number", "patient id"]
      },
      {
        "name": "Insurance_ID",
        "regex": "\\b[A-Z0-9]{8,15}\\b",
        "category": "Insurance ID",
        "context_keywords": ["insurance", "member id", "policy number", "subscriber id"]
      }
    ],
    "phrases": [
      "patient name is",
      "patient's name is",
      "the patient is",
      "dob is",
      "date of birth is",
      "medical record number",
      "mrn is",
      "insurance member id",
      "member id is",
      "policy number is",
      "subscriber id is",
      "hospital identifier"
    ]
  },
  "medical_terms": {
    "diagnosis": [
      "diagnosis",
      "diagnosed with",
      "condition",
      "disease",
      "disorder",
      "syndrome",
      "illness"
    ],
    "lab_results": [
