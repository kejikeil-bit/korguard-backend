  },
  "severity_rules": {
    "red": {
      "description": "PHI detected - identifiers within proximity of medical content"
    },
    "orange": {
      "description": "De-identified medical content - conditions, notes, labs without identifiers"
    },
    "green": {
      "description": "Generic healthcare concepts with no patient inference"
    }
  }
}

